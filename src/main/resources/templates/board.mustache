{{>layout/header}}
<h1>게시판</h1>
<div class="table">
    <table>
        <thead>
            <tr>
                <th>글번호</th>
                <th>제목</th>
                <th>작성자</th>
                <th>작성일</th>
                <th>조회수</th>
            </tr>
        </thead>
        <tbody>
        {{#posts}}
            <tr>
                <td>{{id}}</td>
                <td>{{title}}</td>
                <td>{{author}}</td>
                <td>{{createdDate}}</td>
                <td>{{readNum}}</td>
            </tr>
        {{/posts}}
        </tbody>
    </table>
</div>
<br>
<button type="button" class="btn btn-primary" id="posts-save-btn">글 등록</button>

{{>layout/footer}}